{
    "manifest_version": 3,
    "name": "Acundakonuk Easy Scrape",
    "version": "1.0",
    "description": "Acundakonuk Easy Scrape by Erdal Acundakonuk",
    "permissions": ["activeTab", "scripting","storage","scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "logo.jpg",
      "48": "logo.jpg",
      "128": "logo.jpg"
    },
    "host_permissions": [
      "https://www.google.com/maps/*",
      "https://yandex.com/maps/*"
    ]
  }
  